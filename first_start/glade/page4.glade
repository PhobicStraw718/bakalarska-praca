<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.0 -->
  <object class="GtkBox" id="page4">
        <property name="name">welcome_box</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkButton" id="back_software_button">
            <property name="name">next_page_button</property>
            <property name="can_focus">False</property>
            <property name="receives_default">True</property>
            <signal name="clicked" handler="back" swapped="no"/>
            <!--styles-->
            <style>
                <class name="back_button"/>
            </style>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkFixed">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="halign">center</property>
            <property name="valign">center</property>
            <child>
              <object class="GtkLabel" id="middle_label_software_page">
                <property name="name">middle_label_software_page</property>
                <property name="width_request">300</property>
                <property name="height_request">300</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="halign">center</property>
                <property name="valign">center</property>
              </object>
              <packing>
                <property name="y">40</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="top_label_software_page">
                <property name="name">top_label_software_page</property>
                <property name="width_request">300</property>
                <property name="height_request">380</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>

                  <!--styles-->
                <style>
                    <class name="card_border"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="text_label_software_page">
                <property name="name">bottom_label_software_page</property>
                <property name="width_request">300</property>
                <property name="height_request">40</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="label" translatable="yes">Some software to install</property>
              </object>
              <packing>

              </packing>
            </child>
            <child>
              <object class="GtkSwitch" id="switch_software_1">
                <property name="name">switch_software_1</property>
                <property name="width_request">1</property>
                <property name="height_request">10</property>
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="halign">center</property>
                <property name="valign">center</property>
              </object>
              <packing>
                <property name="x">180</property>
                <property name="y">60</property>
              </packing>
            </child>
            <child>
              <object class="GtkLabel" id="label_software_1">
                <property name="name">label_software_1</property>
                <property name="width_request">100</property>
                <property name="height_request">30</property>
                <property name="visible">True</property>
                <property name="can_focus">False</property>
                <property name="label" translatable="yes">VNC</property>
              </object>
              <packing>
                <property name="x">30</property>
                <property name="y">60</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="expand">True</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="next_software_button">
            <property name="name">next_page_button</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <signal name="clicked" handler="next" swapped="no"/>
            <!--styles-->
            <style>
                <class name="next_button"/>
            </style>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="pack_type">end</property>
            <property name="position">2</property>
          </packing>
        </child>
  </object>
</interface>